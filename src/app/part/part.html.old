<div class="row part">  
    <div class="bread-crum">
        <span class="resultBar">
            <span>{{part.partData.classDesc}} : </span>
            <span>{{part.partData.lineDesc}} : </span>
            <span>{{part.partData.partDesc}}: </span>
            <a>{{part.partData.partNumber}},</a>
            <a>{{part.partData.partDesc}}</a>
        </span>
    </span>
    <div style="clear:fix "> </div>
    </div>
    <div class="part-content">
        <div class="imagePart">
            <div class="partHeading">
                <p>{{part.partData.partNumber}}, {{part.partData.classDesc}}, {{part.partData.partDesc}}</p>
            </div>
            <div class="row image">
                <div class="col-md-4">
                    <image-zoom src="assets/images/u-joint.jpg" zoom-index="5" lens-dimensions="{{part.lensDimensions}}"></image-zoom>
                </div>
                <div class="col-md-4">
                    <table class="table table-striped">
                        <tr data-ng-repeat="(key,val) in part.partData.attrs" >
                            <td>{{key}}</td>
                            <td>{{val}}</td>
                        </tr>
                    </table>
                </div>
                <div class="col-md-4">
                     <!--<div class="add-to-cart">
                       <button class="btn btn-primary single-button" uib-dropdown-toggle>
                            <p>Add To Wishlist<span class="caret"></span></p>
                        </button>
                    </div>-->
                    <div>
                        
                    </div>
                </div>
            </div>
        </div>
        
        <div class="pairs">
            <div class="pairHeading">
                <p>Pairs With</p>
            </div>
            <div class="row">
            </div>
        </div>
        
        <div class="youAlsoBought">
            <div class="pairHeading">
                <p>You Also Bought</p>
            </div>
            <div class="row">
            </div>
        </div>
        
        <div class="identify">
            <div class="pairHeading">
                <p>Identify Your Part</p>
            </div>
            <div class="row">
            </div>
        </div>
        
        <div class="explodedView">
            <div class="pairHeading">
                <p>Exploded View</p>
            </div>
            <div class="row">
            </div>
        </div>
        
        <div class="discussions">
            <div class="pairHeading">
                <p>Popular Discussion</p>
            </div>
            <div class="row">
               
            </div>
        </div>
   </div>
</div>